import numpy as np
a = np.array([2, 4, 4, 4, 5, 5, 7, 9, 6, 8])
b = np.array([1, 3, 5, 2, 4, 6, 8, 7, 5, 9])
m = len(a)
mean_a = np.sum(a) / m
mean_b = np.sum(b) / m
covariance_value = np.sum((a - mean_a) * (b - mean_b)) / m
print(f"Vector a: {a}")
print(f"Vector b: {b}")
print(f"Covariance between a and b: {covariance_value}")


